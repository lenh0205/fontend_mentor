{
  "name": "social-proof-section-master",
  "author": "chi_lenh",
  "version": "0.0.1",
  "description": "frontend mentor social proof",
  "main": "server.js",
  "scripts": {
    "babel": "./node_modules/.bin/babel src --out-dir build --watch --ignore '**/index.scss'",
    "bundle": "watchify build/index.js -o bundle.js",
    "css": "sass --watch src/index.scss index.css",
    "watch": "concurrently \"npm run babel\" \"npm run bundle\" \"npm run css\"",
    "server": "nodemon --config nodemon.json"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "sass": "^1.59.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "browser-sync": "^2.28.3",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.21",
    "watchify": "^4.0.0"
  }
}
